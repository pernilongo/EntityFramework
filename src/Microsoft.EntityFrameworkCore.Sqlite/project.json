{
  "version": "1.0.0-*",
  "description": "SQLite data store for Entity Framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Relational": "1.0.0-*",
    "Microsoft.Data.Sqlite": "1.0.0-*"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "Microsoft.EntityFrameworkCore.Sqlite.SqliteStrings": "Properties/SqliteStrings.resx"
  },
  "resource": [
    "Properties/Microsoft.EntityFrameworkCore.Sqlite.rd.xml"
  ],
  "frameworks": {
    "net451": {},
    "dotnet5.4": {
      "imports": "portable-net452+win81",
      "dependencies": {
        "System.IO.FileSystem": "4.0.1-*"
      }
    }
  }
}