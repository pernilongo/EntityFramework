{
  "version": "7.0.0-*",
  "description": "Shared Design-time Entity Framework components for relational data stores.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "EntityFramework.Relational": "7.0.0-*"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "EntityFramework.Relational.Design.RelationalDesignStrings": "Properties/RelationalDesignStrings.resx"
  },
  "frameworks": {
    "net451": { },
    "dotnet5.4": { },
    "netcore50": {
      "dependencies": {
        "System.Diagnostics.Tools": "4.0.1-*"
      }
    }
  }
}
