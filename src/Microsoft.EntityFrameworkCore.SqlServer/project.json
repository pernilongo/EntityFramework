{
  "version": "1.0.0-*",
  "description": "Microsoft SQL Server data store for Entity Framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Relational": "1.0.0-*"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "Microsoft.EntityFrameworkCore.SqlServer.SqlServerStrings": "Properties/SqlServerStrings.resx"
  },
  "resource": [
    "Properties/Microsoft.EntityFrameworkCore.SqlServer.rd.xml"
  ],
  "frameworks": {
    "net451": {},
    "dotnet5.4": {
      "imports": "portable-net452+win81",
      "dependencies": {
        "System.Data.SqlClient": "4.0.0-*",
        "System.Threading.Thread": "4.0.0-*"
      }
    }
  }
}